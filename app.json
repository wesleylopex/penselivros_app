{
  "expo": {
    "name": "Pense Livros",
    "slug": "penselivros_app",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4285F4"
    },
    "updates": {
